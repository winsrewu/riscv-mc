import ./num_to_binary_array.js
import ./argument_parser.js
import ./bitops_t.mcbt

# see bitops_t.mcbt for the implementation of bitops

function onload minecraft:load {
	scoreboard objectives add bitops dummy

	REPEAT (0, 30) as i {
		scoreboard players set #<%2 ** i%> bitops <%2 ** i%>
	}
	scoreboard players set #n1 bitops -1
}